{
    "name": "lightsaml/sp-bundle",
    "license": "MIT",
    "type": "symfony-bundle",
    "description": "Light SAML2 SP Symfony Bundle",
    "homepage": "http://www.lightsaml.com/SP-Bundle/",
    "autoload": {
        "psr-0": {
            "LightSaml\\SpBundle\\Tests\\": "tests/",
            "LightSaml\\SpBundle\\": "src/"
        }
    },
    "require": {
        "php": ">=5.5.1",
        "symfony/framework-bundle": "~2.6",
        "symfony/security-bundle": "~2.6",
        "lightsaml/symfony-bridge": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.6",
        "symfony/browser-kit": "~2.3",
        "symfony/css-selector": "~2.3",
        "symfony/dom-crawler": "~2.3",
        "symfony/expression-language": "~2.6",
        "symfony/finder": "~2.3",
        "symfony/monolog-bundle": "~2.3",
        "symfony/twig-bundle": "~2.3",
        "satooshi/php-coveralls": "~0.6"
    },
    "config": {
        "bin-dir": "bin"
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
